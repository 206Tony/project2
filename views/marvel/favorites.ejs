<div class="container">
  <h1 class="text-center">My Heros</h1>
  <p class="text-center">
    <a href="/main">Home</a>
  </p>
  <% character.forEach(function(character) { %>
  <form action="/favorites" method='POST'>
  <input hidden type="text" name="name" value="<%= character.character %>" >
  </form>
    <div class="well">
      <h2><a href='/marvel/<%= character.id %>'><%= character.character %></a></h2>
    </div>
  <% }); %>
</div>