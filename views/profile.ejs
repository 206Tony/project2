<h2>Profile Page</h2>

<p>Hello, <%= currentUser.name %></p>

  <% character.forEach(function(character) {
    <div class="well">
      <h2><a href='http://gateway.marvel.com/v1/public/<%= character.name %> + <%= buildMarvelQuery > '><%= character.name %> </h2>
      <form method="POST" action="/profile">
        <input hidden type="text" name="name" value="<%= character.name %>" >
        <button class="btn btn-primary" type="submit">Add to Favorite Hero</button>
      </form>
    </div>
  <% }); %>
</div> %>

<a href="/">Home</a>
